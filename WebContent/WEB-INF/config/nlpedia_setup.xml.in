	<bean id="PatternEvaluatorFactory"
		  class="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.PatternEvaluatorFactory"
		  factory-method="getInstance"
		  singleton="true">
		  
		<property name="patternEvaluatorMap">
			<map>
				<!-- INSERT THE EVALUATORS IN THE ORDER YOU WANT THEM TO RUN -->
				<!-- You can save serious amounts of time if you order them correctly -->
				<!-- for example: check most common probable mistakes first like patterns which only contain stopwords -->
				<!-- more CPU consuming evaluation then needs only to be run for fewer patterns -->
				<entry key="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.impl.StopWordPatternEvaluator" value-ref="StopWordPatternEvaluator" />
				<entry key="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.impl.PatternLengthEvaluator" value-ref="PatternLengthEvaluator" />
				<entry key="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.impl.StartsWithEvaluator" value-ref="StartsWithEvaluator" />
				<entry key="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.impl.OccurrenceEvaluator" value-ref="OccurrenceEvaluator" />
				<entry key="de.uni_leipzig.simba.boa.backend.entity.pattern.evaluation.impl.DomainAndRangeEvaluator" value-ref="DomainAndRangeEvaluator" />
				<!-- entry key="de.uni_leipzig.simba.boa.backend.pattern.evaluation.impl.UniquePatternEvaluator" value-ref="UniquePatternEvaluator" /-->
			</map>
		</property>
	</bean>
	
